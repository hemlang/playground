// Fibonacci sequence
// Demonstrates recursion and template strings

fn fib(n) {
    if (n <= 1) {
        return n;
    }
    return fib(n - 1) + fib(n - 2);
}

fn main() {
    print("Fibonacci sequence:");
    for (let i = 0; i < 15; i = i + 1) {
        print(`fib(${i}) = ${fib(i)}`);
    }
}

main();
