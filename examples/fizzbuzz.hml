// FizzBuzz
// Classic programming challenge

fn fizzbuzz(n) {
    for (let i = 1; i <= n; i = i + 1) {
        if (i % 15 == 0) {
            print("FizzBuzz");
        } else if (i % 3 == 0) {
            print("Fizz");
        } else if (i % 5 == 0) {
            print("Buzz");
        } else {
            print(i);
        }
    }
}

fn main() {
    fizzbuzz(30);
}

main();
